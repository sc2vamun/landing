	var title = $('title').text();
// 		$('li').removeClass('active');
		if(title.indexOf(' ')+1) {
		    title=title.substring(0,title.indexOf(' '))
		    }
		$("li#"+title).addClass('active');
	
		$('li').click(function(){
				$('li').removeClass('active');
				 $(this).addClass('active');
		 
			})
			

		